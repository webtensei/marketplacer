// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DEV_DATABASE_URL")
}

model User {
  username           Int                @id @unique
  password           String
  created_at         DateTime           @default(now())

  @@map("user")
}

